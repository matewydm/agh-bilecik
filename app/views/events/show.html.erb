<p id="komunikat"><%= flash[:komunikat] %></p>
<p id="notice"><%= notice %></p>

<p>
  <strong>Artist:</strong>
  <%= @event.artist %>
</p>

<p>
  <strong>Description:</strong>
  <%= @event.description %>
</p>

<p>
  <strong>Price Low:</strong>
  <%= @event.price_low %>
</p>

<p>
  <strong>Price High: </strong>
  <%= @event.price_high %>
</p>


<p>
  <strong>Event Date:</strong>
  <%= @event.event_date %>
</p>

<%= render "tickets_list", tickets: @event.tickets %>

<%= render "new_ticket", ticket: Ticket.new(:event_id => @event.id) %>

<td><%= link_to 'Back', events_path, class: 'btn btn-warning btn-xs' %></td>

